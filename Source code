def print_data(name, age, nationality, college, CGPA, internships):
    print("The name of the user is " + name + "." + "The age of the user is " + age + "." "They are of" + nationality +"nationality" + "." + "They are studying in" + college + "and have a CGPA of" + CGPA + "." + "They have an experience of" + internships + "in the industry.")

print_data(input("What is your name:\n"), input ("Enter your age:\n"), input("What is your nationality:\n"), input("Enter your college name:\n"), input("What is your CGPA:\n"), input("Got any experience in industry? (If not say no):\n"))
